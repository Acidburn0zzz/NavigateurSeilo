QDialog,
QMenu,
QTabWidget::pane
{
  border: none;
	background: rgb(253, 216, 53);
}

QMainWindow
{
  background-image: url("images/background.png");
}

QTreeWidget,
QTreeView,
QTableWidget,
QTableView,
QListView,
QListWidget {
  background: rgb(253, 216, 53);
  alternate-background-color: rgba(255, 255, 255, .7);
}

QTextBrowser {
  border: none;
  background: rgba(255, 255, 255,.3);
}

QDialog QLabel
{
	font-size: 12px;
}

QDialog QPushButton
{
	background: rgba(255, 255 ,255, .7);
	border: 2px solid transparent;
	border-radius: 4px;
	padding: 7px 14px;
	font-size: 12px;
}

QDialog QPushButton:hover,
QDialog QPushButton:focus
{
	border-color: black;
}

QDialog QPushButton:focus
{
	background: black;
	color: rgb(253, 216, 53);
}

QSplitterHandle {
    background: rgba(253, 216, 53, 0.50);
}

#navigationbar
{
	border: none;
	background: rgb(253, 216, 53);
	padding: 0px;
	spacing: 0px;
}

#navigationbar::separator
{
	background: rgba(0,0,0,.1);
  height: 1px;
  margin: 0px 3px;
}

#navigationbar QToolButton,
#navigationbar ToolButton
{
  background: rgb(253, 216, 53);
	border: none;
	color: white;
	padding: 0px 7px;
  height: 45px;
	font-size: 12px;
}

#navigationbar QToolButton:hover,
#navigationbar ToolButton:hover
{
	background: rgba(255,255,255,.7);
}

#navigationbar QToolButton:pressed,
#navigationbar ToolButton:pressed,
#navigationbar QToolButton:checked,
#navigationbar ToolButton:checked
{
	background: rgba(255,255,255,.7);
}

#navigationbar ToolButton::menu-indicator
{
	image: url(none.png);
}

#addressbar
{
	border: none;
	background: rgb(253, 216, 53);
	margin: 0px;
}
/*
#navigationbar #addressbar #Icon
{
	border: none;
	background: transparent;
	padding: 10px;
	padding-right: 0px;
	margin: 0px;
}
*/

#navigationbar #addressbar QToolButton,
#navigationbar #addressbar QToolButton:hover
{
	border: none;
	background: transparent;
	font-size: 14px;
	margin: 0px;
	padding: 0px;
}

#navigationbar #addressbar QLabel
{
	border: none;
	padding: 0px 10px;
	font-size: 18px;
}

QMenu::separator
{
	background: rgba(0,0,0,.1);
  height: 1px;
  margin: 0px 3px;
}

QMenu::item:selected
{
	background: rgb(0, 136, 249) repeat-x;
  color: white;
}

QLineEdit
{
	padding: 4px 6px;
	background: transparent;
	border: 1px solid black;
	font-size: 12px;
	border-radius: 2px;
}

QComboBox {
	border: none;
	background: rgba(255,255,255,.4);;
	padding: 1px 23px 1px 3px;
	min-width: 6em;
}

QComboBox::drop-down {
	border: none;
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 20px;
}

QComboBox QAbstractItemView{
	border: none;
	background-color: rgba(255,255,255,.7);;
	color: rgba(0,0,0,.4);
	selection-background-color: rgba(255,255,255,.4);
	selection-color: #4f4f4f;
}

QCheckBox,
QRadioButton
{
	spacing: 7px;
	font-size: 12px;
}

QCheckBox:hover,
QRadioButton:hover
{
	color: rgb(253, 216, 53);
}

QCheckBox:disabled,
QRadioButton:disabled
{
	color: rgba(0,0,0,.3);
}

QCheckBox::indicator,
QRadioButton::indicator
{
	width: 16px;
	height: 16px;
	border: 1px solid black;
	background: transparent;
	padding: 1px;
	border-radius: 2px;

	border: none;
	background: rgba(0,0,0,.1);
	width: 18px;
	height: 18px;
}

QCheckBox::indicator:disabled,
QRadioButton::indicator:disabled
{
	border-color: rgba(255,255,255,.3);
}

QRadioButton::indicator
{
	border-radius: 9px;
	padding: 3px;
	width: 13px;
	height: 13px;
}

QCheckBox::indicator:hover,
QRadioButton::indicator:hover
{
	background: rgb(253, 216, 53);
}

QCheckBox::indicator:checked
{
	image: url(images/other/check.png);
	padding: 4px;
	width: 12px;
	height: 12px;
}

QRadioButton::indicator:checked
{
	image: url(images/other/round.png);
}


QScrollBar:vertical
{
	border: none;
	background: transparent;
	width: 17px;
	margin: 0px;
}

QScrollBar::handle:vertical
{
	background: rgba(0, 0, 0, .2);
	min-height: 50px;
	margin-right: 2px;
	border-radius: 7px;
	border: none;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical
{
	border: none;
	background: transparent;
	height: 0px;
	subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical
{
	border: none;
	width: 0px;
	height: 0px;
	background: transparent;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical
{
	background: none;
}

/* HORIZONTAL */
QScrollBar:horizontal
{
	border: none;
	background: transparent;
	height: 17px;
	margin: 0px;
}

QScrollBar::handle:horizontal
{
	background: rgba(0, 0, 0, .2);
	min-width: 50px;
	margin-bottom: 2px;
	border-radius: 7px;
	border: none;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal
{
	subcontrol-origin: margin;
	width: 0px;
	border: none;
	background: transparent;
}

QScrollBar:left-arrow:horizontal,
QScrollBar::right-arrow:horizontal
{
	border: none;
	width: 0px;
	height: 0px;
	background: transparent;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal
{
	background: none;
}
