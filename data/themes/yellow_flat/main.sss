#tabbarwidget * {
    background: rgb(255, 235, 59);
}

#tabbar {
	height: 28px;
}

QTabBar {
  border: none;
  background: rgb(255, 235, 59);
}

QTabBar::tab
{
    height: 28px;
}

QTabBar::tab:selected
{
    background: rgb(253, 216, 53);
}

QTabBar::tab:!selected
{
    background: transparent
}

QTabBar::tab:!selected:hover
{
    background: rgb(0, 136, 249) repeat-x;
}

#tabs-closebutton:hover
{
    border-radius: 0px;
    background-color: #C62828;
}

